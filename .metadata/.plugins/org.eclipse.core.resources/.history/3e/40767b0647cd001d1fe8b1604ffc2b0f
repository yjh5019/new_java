		System.out.printf("HanBlackForeword[%s]\n",HanBlankForeword("한글abcd",15));
		System.out.printf("HanBlackForeword[%s]\n",HanBlankForeword("한글한글aa",15));
		System.out.printf("HanBlackForeword[%s]\n",HanBlankForeword("한글aa",15));
